<?xml version = '1.0' encoding = 'UTF-8'?>
<struts-config>
	<form-beans>
		<form-bean name="dynamicForm" type="com.cnpc.jcdp.mvc.action.DynaActionForm" />
	</form-beans>
	<action-mappings>
	
		<!--自定义操作证申请审批操作 -->
		<action path="/dmsManager/device/checkOPCApplyInfo" type="com.cnpc.jcdp.mvc.action.WSAction"
			scope="request" name="dynamicForm">
			<serviceCall serviceName="WFCommonSrv" operationName="getWFProcessInfo" />
			<forward name="success"	path="/dmsManager/opcard/OperationCertificateApplyReport.jsp" redirect="false"/>
		</action>
		<!--获取完好率（按量计算）图表数据 -->
		<action path="/dms/device/getAmountWhlChartData" type="com.bgp.mcs.web.pm.gmsChart.action.querySeriesXMLDataAction"
			scope="request">
			<serviceCall serviceName="DeviceAnalSrv" operationName="getAmountWhlChartData" />
		</action>
		<!--获取利用率图表数据  start-->
		<action path="/dms/device/getUseRate" type="com.bgp.mcs.web.pm.gmsChart.action.querySeriesXMLDataAction"
			scope="request">
			<serviceCall serviceName="DeviceUseRateSrv" operationName="getUseRate" />
		</action> 	
		<!--获取利用率图表数据  start-->
		<action path="/dms/device/getYearUseRate" type="com.bgp.mcs.web.pm.gmsChart.action.querySeriesXMLDataAction"
			scope="request">
			<serviceCall serviceName="DeviceUseRateSrv" operationName="getYearUseRateNew" />
		</action> 
		
		<!--获取非地震仪器利用率图表二级钻取数据 -->
		<action path="/dms/device/getSecondLevelUseRate" type="com.bgp.mcs.web.pm.gmsChart.action.querySeriesXMLDataAction"
			scope="request">
			<serviceCall serviceName="DeviceUseRateSrv" operationName="getSecondLevelUseRate" />
		</action> 
		<!-- 获取震源利用率图表二级钻取数据-->
		<action path="/dms/device/getSecondLevelZyUseRate" type="com.bgp.mcs.web.pm.gmsChart.action.querySeriesXMLDataAction"
			scope="request">
			<serviceCall serviceName="DeviceUseRateSrv" operationName="getSecondLevelZyUseRate" />
		</action>
		<!-- 获取检波器利用率图表二级钻取数据-->
		<action path="/dms/device/getSecondLevelJbqUseRate" type="com.bgp.mcs.web.pm.gmsChart.action.querySeriesXMLDataAction"
			scope="request">
			<serviceCall serviceName="DeviceUseRateSrv" operationName="getSecondLevelJbqUseRate" />
		</action>
		<!--获取地震仪器利用率图表二级钻取数据 -->
		<action path="/dms/device/getSecondLevelInsUseRate" type="com.bgp.mcs.web.pm.gmsChart.action.querySeriesXMLDataAction"
			scope="request">
			<serviceCall serviceName="DeviceUseRateSrv" operationName="getSecondLevelInsUseRate" />
		</action> 
		<!--获取地震仪器利用率图表三级钻取数据 -->
		<action path="/dms/device/getThirdLevelInsUseRate" type="com.bgp.mcs.web.pm.gmsChart.action.querySeriesXMLDataAction"
			scope="request">
			<serviceCall serviceName="DeviceUseRateSrv" operationName="getThirdLevelInsUseRate" />
		</action> 
		<!--获取利用率图表数据  end-->
		
		<!--获取新度系数图表数据  start-->
		<action path="/dms/device/getNewExtent" type="com.bgp.mcs.web.pm.gmsChart.action.querySeriesXMLDataAction"
			scope="request">
			<serviceCall serviceName="DeviceNewExtentSrv" operationName="getNewExtent" />
		</action> 
		<!--获取非地震仪器新度系数图表二级钻取数据 -->
		<action path="/dms/device/getSecondLevelNewExtent" type="com.bgp.mcs.web.pm.gmsChart.action.querySeriesXMLDataAction"
			scope="request">
			<serviceCall serviceName="DeviceNewExtentSrv" operationName="getSecondLevelNewExtent" />
		</action> 
		<!-- 获取震源新度系数 -->
		<action path="/dms/device/getSecondLevelZyNewExtent" type="com.bgp.mcs.web.pm.gmsChart.action.querySeriesXMLDataAction"
			scope="request">
			<serviceCall serviceName="DeviceNewExtentSrv" operationName="getSecondLevelZyNewExtent" />
		</action>
		<!-- 获取检波器新度系数 -->
		<action path="/dms/device/getSecondLevelJbqNewExtent" type="com.bgp.mcs.web.pm.gmsChart.action.querySeriesXMLDataAction"
			scope="request">
			<serviceCall serviceName="DeviceNewExtentSrv" operationName="getSecondLevelJbqNewExtent" />
		</action> 
		<!--获取地震仪器新度系数图表二级钻取数据 -->
		<action path="/dms/device/getSecondLevelInsNewExtent" type="com.bgp.mcs.web.pm.gmsChart.action.querySeriesXMLDataAction"
			scope="request">
			<serviceCall serviceName="DeviceNewExtentSrv" operationName="getSecondLevelInsNewExtent" />
		</action> 
		<!--获取地震仪器新度系数图表三级钻取数据 -->
		<action path="/dms/device/getThirdLevelInsNewExtent" type="com.bgp.mcs.web.pm.gmsChart.action.querySeriesXMLDataAction"
			scope="request">
			<serviceCall serviceName="DeviceNewExtentSrv" operationName="getThirdLevelInsNewExtent" />
		</action>
		<!--获取新度系数图表数据  end-->
		
		<!--获取设备出勤率图表数据 -->
		<action path="/dms/device/getAtteChartData" type="com.bgp.mcs.web.pm.gmsChart.action.querySeriesXMLDataAction"
			scope="request">
			<serviceCall serviceName="DeviceAtteSrv" operationName="getAtteChartData" />
		</action>
		<!--获取所在项目设备出勤率图表数据 -->
		<action path="/dms/device/getProDevAtteChartData" type="com.bgp.mcs.web.pm.gmsChart.action.querySeriesXMLDataAction"
			scope="request">
			<serviceCall serviceName="DeviceAtteSrv" operationName="getProDevAtteChartData" />
		</action>
		
		<!--OA系统挂审批链接 -->
		<action path="/oa_process_list" type="com.cnpc.jcdp.web.auth2.action.LoginAction"
			scope="request">
			<serviceCall serviceName="DeviceAnalSrv"
				operationName="OAProcessList" />
			<forward name="success" path="/oa/getProcessList.srq?isDone=0" />
			
			<forward name="failed" path="/login.jsp" />
		</action>
		<action path="/oa/getProcessList" type="com.cnpc.jcdp.mvc.action.WSAction" scope="request">
			<serviceCall serviceName="WFCommonSrv" operationName="getCustomConfigInfo"  />
			<forward name="success" path="/pm/oa/oaProcess.jsp" />
		</action>
		
		<action path="/oa/moreProcessList" type="com.cnpc.jcdp.mvc.action.WSAction" scope="request">
			<serviceCall serviceName="WFCommonSrv" operationName="getCustomConfigInfo"  />
			<forward name="success" path="/pm/oa/oaProcess1.jsp" />
		</action>
	</action-mappings>
</struts-config>
