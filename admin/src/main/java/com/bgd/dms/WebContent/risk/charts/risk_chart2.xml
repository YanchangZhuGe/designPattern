    <dataset
        seriesName="极高风险"
        color="FF0000" >

        <set value="0.6" />

        <set value="0.1" />

        <set value="0.2" />

        <set value="0.2" />

        <set value="0.2" />

        <set value="0.3" />
    </dataset>

    <dataset
        alpha="1"
        anchorBgColor="FFFFFF"
        anchorRadius="5"
        anchorSides="4"
        renderAs="Line"
        seriesName="风险值" >

        <set value="0.7" />

        <set value="0.1" />

        <set value="0.2" />

        <set value="0.3" />

        <set value="0.5" />

        <set value="0.3" />
