    showValues="0"
    useRoundEdges="0"
    yAxisMaxValue="1.0"
    yAxisName="风险值"
    yAxisNameWidth="16" >

    <categories>

        <category label="环保风险" />

        <category label="气候环境风险" />

        <category label="成本投入进度风险" />

        <category label="管网工程风险" />

        <category label="安全消防风险" />

        <category label="排水等辅助工程风险" />
    </categories>

    <dataset
        seriesName="低度风险"
        color="00FF00" >

