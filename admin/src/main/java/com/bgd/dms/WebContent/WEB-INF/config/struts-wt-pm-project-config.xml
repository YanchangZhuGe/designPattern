<?xml version = '1.0' encoding = 'UTF-8'?>
<struts-config>
	<form-beans>
		<form-bean name="dynamicForm" type="com.cnpc.jcdp.mvc.action.DynaActionForm" />
	</form-beans>
	<action-mappings>
		
		
		
		<!-- 计划日效 保存 -->
    	<action path="/wt/pm/dailyPlan/saveOrUpdateWtDailyPlan" type="com.cnpc.jcdp.mvc.action.WSAction"
			scope="request">
			<serviceCall serviceName="WsDailyReportPlanSrv"
				operationName="saveOrUpdateWtDailyPlan" />
			<forward name="success" path="/wt/pm/planManager/singlePlan/planning/dailyPlanList.jsp"/>
		</action>
		
		<!-- 计划日效 重新加载 -->
    	<action path="/wt/pm/dailyPlan/refreshDailyPlanWt" type="com.cnpc.jcdp.mvc.action.WSAction"
			scope="request">
			<serviceCall serviceName="WsDailyReportPlanSrv"
				operationName="refreshDailyPlanWt" />
			<forward name="success" path="/wt/pm/planManager/singlePlan/planning/dailyPlanList.jsp"/>
		</action>
		
		<!-- 技术管理  设计参数 -->
    	<action path="/wt/tm/parameter/saveWtWorkMethod" type="com.cnpc.jcdp.mvc.action.WSAction"
			scope="request">
			<serviceCall serviceName="WtWorkMethodSrv"
				operationName="saveWtWorkMethod" />
			<forward name="success" path="/wt/tm/parameter/technicalParameter.jsp"/>
		</action>
	
		
		<!-- 生产管理 动迁申请-->
		<action path="/wt/relocation/toSaveRelocation" type="com.cnpc.jcdp.mvc.action.jcdp.WSUploadAction"
			scope="request" name="dynamicForm">
			<serviceCall serviceName="WtRelocationSrv"
				operationName="saveRelocationData" />
				<!-- 
			<forward name="success" path="/wt/relocation/relocationIndex.jsp" /> -->
			<forward name="success" path="/rm/dm/common/close_page.jsp" />
			
		</action>
		<!-- 生产管理 多项目 动迁审批-->
		<action path="/wt/bpm/toGetSelfProcessList" type="com.cnpc.jcdp.mvc.action.WSAction" scope="request">
			<serviceCall serviceName="WTCommonSrv" operationName="getCustomConfigInfo"  />
			<forward name="success" path="/wt/relocation/multiProject/commonSelfProcessList.jsp" />
		</action>
	
		
	</action-mappings>
</struts-config>
