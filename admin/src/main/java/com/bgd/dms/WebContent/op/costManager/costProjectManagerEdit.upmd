  <Options/>
  <Codes/>
  <EntityFields>
    <field name="gp_cost_project_id" isShow="Hide">id</field>
    <field name="project_info_no" isShow="Hide" defaultValue="$PARAM.projectInfoNo">id</field>
    <field name="parent_id" isShow="Hide" defaultValue="$PARAM.parentId">id</field>
    <field name="cost_type" isShow="Hide" defaultValue="$PARAM.costType">id</field>
    <field name="bsflag" isShow="Hide" defaultValue="0">bsflag</field>
    <field name="cost_name" maxLength="100"  nonEmpty="true">费用名称</field>
    <field name="cost_desc" maxLength="500">费用描述</field>
