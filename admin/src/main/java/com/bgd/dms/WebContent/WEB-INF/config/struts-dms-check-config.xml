<?xml version = '1.0' encoding = 'UTF-8'?>
<struts-config>
	<form-beans>
		<form-bean name="dynamicForm" type="com.cnpc.jcdp.mvc.action.DynaActionForm" />
	</form-beans>
	<action-mappings>
		<!--采购订单的采购方式、采购类别 -->
		<action path="/rm/dm/toGetJsonPurch" type="com.bgp.gms.service.rm.dm.action.GetDeviceAction"
			scope="request" name="dynamicForm">
			<serviceCall serviceName="PurchaseSrv" operationName="getPurchTypeInfo" />
		</action>
		<!--保存修改验收通知信息 -->
		<action path="/dmsManager/check/saveOrUpdateCheckConfInfo" type="com.cnpc.jcdp.mvc.action.jcdp.WSUploadAction"
			scope="request" name="dynamicForm">
			<serviceCall serviceName="CheckDevNotice" operationName="saveOrUpdateCheckConfInfo" />
			<forward name="success"	path="/dmsManager/common/easyui_close_page.jsp" redirect="false" />
		</action>
		<!--保存修改验收准备信息 -->
		<action path="/dmsManager/check/saveOrUpdateCheckReadyInfo" type="com.cnpc.jcdp.mvc.action.jcdp.WSUploadAction"
			scope="request" name="dynamicForm">
			<serviceCall serviceName="CheckDevReady" operationName="saveOrUpdateCheckReadyInfo" />
			<forward name="success"	path="/dmsManager/common/easyui_close_page.jsp" redirect="false" />
		</action>
		<!--保存修改验收信息 -->
		<action path="/dmsManager/check/saveOrUpdateCheckDoInfo" type="com.cnpc.jcdp.mvc.action.jcdp.WSUploadAction"
			scope="request" name="dynamicForm">
			<serviceCall serviceName="CheckDevDo" operationName="saveOrUpdateCheckDoInfo" />
			<forward name="success"	path="/dmsManager/common/easyui_close_page.jsp" redirect="false" />
		</action>
		<!--保存修改供货商评价-->
		<action path="/dmsManager/check/saveOrUpdateCheckCsiInfo" type="com.cnpc.jcdp.mvc.action.WSAction"
			scope="request" name="dynamicForm">
			<serviceCall serviceName="CheckPersonDev" operationName="saveOrUpdateCheckCsiInfo" />
			<forward name="success"	path="/dmsManager/check/docheck.jsp" redirect="false" />
		</action>
		<!--保存修改问题信息-->
		<action path="/dmsManager/check/saveOrUpdateCheckQuestionInfo" type="com.cnpc.jcdp.mvc.action.WSAction"
			scope="request" name="dynamicForm">
			<serviceCall serviceName="CheckDevQuestion" operationName="saveOrUpdateCheckQuestionInfo" />
			<forward name="success"	path="/dmsManager/common/easyui_close_page.jsp" redirect="false" />
		</action>
		
		
		<!-- 设备原值 -->
		<action path="/dmsManager/zhuanzi/getDevicebf" type="com.bgp.mcs.web.pm.gmsChart.action.querySeriesXMLDataAction"
			scope="request">
			<serviceCall serviceName="DeviceBF" operationName="getDevicebf" />
		</action>
		<!-- 油料消耗 -->
		<action path="/dmsManager/zhuanzi/getDeviceTotalMoney" type="com.bgp.mcs.web.pm.gmsChart.action.querySeriesXMLDataAction"
			scope="request">
			<serviceCall serviceName="DeviceBF" operationName="getDeviceTotalMoney" />
		</action>
		
		<!-- 行驶里程 -->
		<action path="/dmsManager/zhuanzi/getDeviceMileage" type="com.bgp.mcs.web.pm.gmsChart.action.querySeriesXMLDataAction"
			scope="request">
			<serviceCall serviceName="DeviceBF" operationName="getDeviceMileage" />
		</action>
		
		<!-- 参与项目 -->
		<action path="/dmsManager/zhuanzi/getDeviceCount" type="com.bgp.mcs.web.pm.gmsChart.action.querySeriesXMLDataAction"
			scope="request">
			<serviceCall serviceName="DeviceBF" operationName="getDeviceCount" />
		</action>
		
		<!--保存修改出入库详细信息 -->
		<action path="/dmsManager/safekeeping/saveOrUpdateKeepingConfInfo" type="com.cnpc.jcdp.mvc.action.jcdp.WSUploadAction"
			scope="request" name="dynamicForm">
			<serviceCall serviceName="DevKeeping" operationName="saveOrUpdateKeepingConfInfo" />
			<forward name="success"	path="/dmsManager/safekeeping/easyui_close_page.jsp" redirect="false" />
		</action>
		
		<!-- 获取自定义列表数据-->
		<action path="/dmsManager/safekeeping/queryKeepingConfInfoList" type="com.bgp.gms.service.rm.dm.action.EasyUIDataGridAction" 
			scope="request">
		</action>
		
		<!-- 异步获取map数据  -->
		<action path="/dmsManager/safekeeping/getKeepingConfInfo" type="com.bgp.gms.service.rm.dm.action.AjaxRetMapAction" 
			scope="request">
		</action>
		
		<!-- 获取台账自定义列表数据-->
		<action path="/dmsManager/safekeeping/queryKeepingDevList" type="com.bgp.gms.service.rm.dm.action.EasyUIDataGridAction" 
			scope="request">
		</action>
		
		<!-- 异步获取台账map数据  -->
		<action path="/dmsManager/safekeeping/getKeepingDevInfo" type="com.bgp.gms.service.rm.dm.action.AjaxRetMapAction" 
			scope="request">
		</action>
		
		<!-- 获取保养列表数据-->
		<action path="/dmsManager/safekeeping/queryMaintainDevList" type="com.bgp.gms.service.rm.dm.action.EasyUIDataGridAction" 
			scope="request">
		</action>
		
		<!-- 获取自定义列表数据-->
		<action path="/dmsManager/safekeeping/getMaintainDevInfo" type="com.bgp.gms.service.rm.dm.action.EasyUIDataGridAction" 
			scope="request">
		</action>
		
		<!-- 异步获取台账map数据  -->
		<action path="/dmsManager/safekeeping/getQZMaintain" type="com.bgp.gms.service.rm.dm.action.AjaxRetMapAction" 
			scope="request">
		</action>
		
		<!--保存修改出入库详细信息 -->
		<action path="/dmsManager/safekeeping/saveDeviceAccMaintenancePlan" type="com.cnpc.jcdp.mvc.action.WSAction"
			scope="request" name="dynamicForm">
			<serviceCall serviceName="DevMaintainList" operationName="saveDeviceAccMaintenancePlan" />
			<forward name="success"	path="/dmsManager/safekeeping/easyui_close_page.jsp" redirect="false" />
		</action>
		
		<!--保存修改油料消耗详细信息 -->
		<action path="/dmsManager/safekeeping/saveDeviceExpendInfo" type="com.cnpc.jcdp.mvc.action.WSAction"
			scope="request" name="dynamicForm">
			<serviceCall serviceName="DevRepairList" operationName="saveOrUpdateExpendInfo" />
			<forward name="success"	path="/dmsManager/safekeeping/easyui_close_page.jsp" redirect="false" />
		</action>
		<!-- 导出定人定机Excel -->
		<action path="/dmsManager/safekeeping/toGetDevExcel" type="com.bgp.gms.service.rm.dm.action.GetDevExcelFromAction"
			scope="request" name="dynamicForm">
			<serviceCall serviceName="DevOperatorList" operationName="getDevArchiveInfo" />
		</action>
		
		<!-- 异步获取保养详细数据  -->
		<!-- <action path="/dmsManager/safekeeping/getMaintainDevInfo" type="com.bgp.gms.service.rm.dm.action.AjaxRetMapAction" 
			scope="request"> -->
	</action-mappings>
</struts-config>
