<?xml version="1.0" encoding="GBK"?>

<SAIS_EAW_TL>
  <PageTitle>操作列表</PageTitle>
  <PagerAction>EntityAndItems</PagerAction>
  <Buttons>
    <Button Type="Add" funCode="">addOp.apmd?parent_id=$PARAM.parent_id</Button>
    <Button Type="Edit" funCode="">editOp.upmd</Button>
    <Button Type="Delete" funCode="">DELETE FROM sys_config_operation_dms WHERE opt_id='{id}'</Button>
    <Button Type="Back" funCode=""></Button>
  </Buttons>
  <QuerySQL>
    <sql>SELECT * FROM sys_config_operation_dms WHERE service_id='$PARAM.parent_id'</sql>
    <fields>
      <field name="opt_id" type="TEXT"></field>
      <field name="service_id" type="TEXT"></field>
      <field name="opt_name" type="TEXT"></field>
      <field name="opt_cnname" type="TEXT"></field>
      <field name="is_monitor" type="TEXT"></field>
      <field name="is_log" type="TEXT"></field>
      <field name="create_time" type="D"></field>
      <field name="creator" type="TEXT"></field>
      <field name="memo" type="TEXT"></field>
      <field name="impl_type" type="TEXT"></field>
      <field name="query_sql" type="TEXT"></field>
      <field name="info_tag" type="TEXT"></field>
      <field name="field_map" type="TEXT"></field>
      <field name="data_auth_key" type="TEXT"></field>
    </fields>
  </QuerySQL>
  <DataList>
		<data exp="&lt;input type='checkbox' name='chx_entity_id' value='{opt_id}'\&gt;"></data>  
    <data exp="&lt;a href=javascript:popWindow('editOp.upmd?id={opt_id}')>{opt_name}&lt;/a>">操作名</data>
    <data exp="{opt_cnname}">中文名称</data>
    <data exp="{create_time}" func="substr,0,10">创建日期</data>
  </DataList>
  <MainEntity>
    <PageTitle>服务信息</PageTitle>
    <EditPMD>editSrv.upmd</EditPMD>
  </MainEntity>
</SAIS_EAW_TL>
