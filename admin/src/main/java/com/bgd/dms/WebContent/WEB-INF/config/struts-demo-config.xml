<?xml version = '1.0' encoding = 'UTF-8'?>
<struts-config>
	<form-beans>
		<form-bean name="dynamicForm" type="com.cnpc.jcdp.mvc.action.DynaActionForm" />
	</form-beans>
	<action-mappings>
		
    	<action path="/demo/FusionChartsDataAction" type="com.bgp.mcs.web.demo.FusionChartsDataAction"
			scope="request">
			<!-- <serviceCall serviceName="TsExcelExportImportSrv"
				operationName="exportExcelData" /> -->
		</action>
		
		<!-- 单炮评价编码 -->
		<action path="/common/portlet/getCategory" type="com.bgp.mcs.web.qua.action.QualityItemsAction"
			name="dynamicForm" scope="request">
			<serviceCall serviceName="PortletSrv" operationName="getCategory" />
		</action>
		
		<!-- 管理员管理 -->
        <action path="/ibp/auth2/saveManager"
			type="com.cnpc.jcdp.web.tcg.ajax.action.AjaxCallAction"
			scope="request">
			<serviceCall serviceName="IBPAuthEntitySrv"
				operationName="saveManager" />
		</action>
		<!-- 登陆页面修改用户密码功能 -->
		<action path="/rm/dm/changePassword/updateHomePassWord" type="com.cnpc.jcdp.mvc.action.WSAction"
			scope="request" name="dynamicForm">
			<serviceCall serviceName="DevCommWtcSrv" operationName="updateHomePassWord" />
			<forward name="success" path="/rm/dm/common/close_msg_page.jsp" />
		</action>   
		<action path="/ibp/auth2/deleteManager"
			type="com.cnpc.jcdp.web.tcg.ajax.action.AjaxCallAction"
			scope="request">
			<serviceCall serviceName="IBPAuthEntitySrv"
				operationName="deleteManager" />
		</action>
		
		<action path="/web/auth/loginOut" type="com.cnpc.jcdp.web.auth.action.LoginOutAction"
			scope="request">
		</action>
		
		<action path="/test/service/handRun"
			type="com.cnpc.jcdp.mvc.action.WSAction"
			scope="request">
			<serviceCall serviceName="TestService"
				operationName="queryProject" />
		</action>
		
		<!-- 	<action path="/test/service/projectP6Syn"
			type="com.cnpc.jcdp.mvc.action.WSAction"
			scope="request">
			<serviceCall serviceName="DevCommInfoSrv"
				operationName="getWHLYTRANS" />
		</action> -->
		
	</action-mappings>
</struts-config>
