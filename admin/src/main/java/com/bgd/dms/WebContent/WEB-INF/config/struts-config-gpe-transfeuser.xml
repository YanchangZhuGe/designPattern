<?xml version = '1.0' encoding = 'UTF-8'?>

<struts-config>
  <action-mappings>
    <action path="/gpe/insertTransferUser"
    		type="com.cnpc.jcdp.mvc.action.WSAction">
    	    <forward name="success" path="/pm/structureUnit/close_page.jsp" redirect="true"/> 
    	    <serviceCall serviceName="TransferUserInfoSrv" 
      		             operationName="addTransUser"/>
    </action>
    
    <action path="/gpe/updateTransferUser"
    		type="com.cnpc.jcdp.mvc.action.WSAction">
    	    <forward name="success" path="/pm/structureUnit/close_page.jsp" redirect="true"/> 
    	    <serviceCall serviceName="TransferUserInfoSrv" 
      		             operationName="updateTransUser"/>
    </action>    
  </action-mappings>
</struts-config>
