<?xml version = '1.0' encoding = 'UTF-8'?>
<struts-config>
	<form-beans>
		<form-bean name="dynamicForm" type="com.cnpc.jcdp.mvc.action.DynaActionForm" />
	</form-beans>
	<action-mappings>
		<!--新增或修改自动评分信息 -->
		<action path="/dms/autoscore/saveOrUpdateAutoScore" type="com.cnpc.jcdp.mvc.action.WSAction"
			scope="request" name="dynamicForm">
			<serviceCall serviceName="AutoScoreSrv" operationName="saveOrUpdateAutoScore" />
			<forward name="success"	path="/dmsManager/common/close_page.jsp" redirect="false" />
		</action>
		<!--新增或修改自动评分关联信息 -->
		<action path="/dms/autoscore/saveOrUpdateAutoScoreRelation" type="com.cnpc.jcdp.mvc.action.WSAction"
			scope="request" name="dynamicForm">
			<serviceCall serviceName="AutoScoreSrv" operationName="saveOrUpdateAutoScoreRelation" />
			<forward name="success"	path="/dmsManager/common/close_page.jsp" redirect="false" />
		</action>
	</action-mappings>
</struts-config>
