<script type="text/javascript">
	cruConfig.contextPath =  "<%=contextPath%>";
	cruConfig.cdtType = 'form';
	
	var project = '<%=projectInfoNo%>';
	function refreshData(){
		if(project ==null || project=='null' || project ==''){
			alert("请选择项目");
			return;
		}
		cruConfig.queryStr = "select t.record_id ,t.title ,t.record_date ,t.record_place ,t.record_content ,t.record_code ,t.record_num ,t.record_master,t.recorder from bgp_qua_meeting_record t where t.bsflag='0' and t.project_info_no ='"+project+"'";
		setTabBoxHeight();
		queryData(1);
	}
	refreshData();
	/* 详细信息 */
	function loadDataDetail(record_id){
		var obj = event.srcElement;
		if(obj.tagName.toLowerCase()=='td'){
			var tr = obj.parentNode;
			tr.cells[0].firstChild.checked =true;
		}
		var sql = "select t.record_id ,t.title ,t.record_date ,t.record_place ,t.record_master ,t.recorder ,"+
		" t.record_content ,t.record_code ,t.record_num "+
		" from bgp_qua_meeting_record t "+
		" where t.bsflag='0' and t.record_id ='"+record_id+"'";
		
		var retObj = syncRequest('Post',cruConfig.contextPath + appConfig.queryListAction,'querySql='+encodeURI(encodeURI(sql)));
		if(retObj!=null && retObj.returnCode =='0'){
			if(retObj.datas!=null && retObj.datas.length>0){
				if(retObj.datas[0]!=null){
					var map = retObj.datas[0];
					document.getElementById("record_id").value = record_id;
					document.getElementById("record_code").value = map.record_code;
					document.getElementById("record_num").value = map.record_num;
					document.getElementById("title").value = map.title;
					document.getElementById("record_date").value = map.record_date;
					document.getElementById("record_place").value = map.record_place;
					document.getElementById("record_master").value = map.record_master;
					document.getElementById("recorder").value = map.recorder;
					document.getElementById("record_content").value = map.record_content;
				}
			}
		}
	}
	/* 修改 */
	function toAdd() {
		popWindow(cruConfig.contextPath+"/qua/sProject/quaFile/recordEdit.jsp");
	}
	function toEdit() {
		var obj = document.getElementsByName("chk_entity_id");
		var objLen= obj.length; 
		var record_id = '';
		if(window.confirm("你确定要修改吗?")){
			for (var i = 0;i< objLen ;i++){   
			    if (obj [i].checked==true) { 
			    	record_id=obj [i].value;
					popWindow(cruConfig.contextPath+"/qua/sProject/quaFile/recordEdit.jsp?record_id="+record_id);
					return;
				}
		  	}   
		} 
		alert("请选择修改的记录!")
	}
	function toDel() {
		var obj = document.getElementsByName("chk_entity_id");
		var objLen= obj.length; 
		var record_id = '';
		if(window.confirm("你确定要删除吗?")){
			var sql = "";
			for (var i = objLen-2;i>0;i--){   
				if (obj [i].checked==true) { 
					record_id=obj [i].value;
					sql =" update bgp_qua_meeting_record t set t.bsflag='1' where t.record_id='"+record_id+"';";
				}   
			} 
			var retObj = jcdpCallService("QualitySrv", "saveQualityBySql", "sql="+sql);
			if(retObj!=null && retObj.returnCode=='0'){
				refreshData();
			}
		}
		
	}
	var selectedTag=document.getElementsByTagName("li")[0];
	function getTab(obj,index) {  
		if(selectedTag!=null){
			selectedTag.className ="";
		}
		selectedTag = obj.parentElement;
		selectedTag.className ="selectTag";
		var showContent = 'tab_box_content'+index;
		
		for(var i=1; j=document.getElementById("tab_box_content"+i); i++){
			j.style.display = "none";
		}
		document.getElementById(showContent).style.display = "block";
	}

	
	function reportShow(record_id){
		
		popWindow('<%=contextPath%>/$bireport/dynamic/rpt/showReport?isRefreshCache=true&reportId=QUA_MEET_RECORD&noLogin=admin&tokenId=admin&KeyId='+record_id,'930:720');
	}
	function frameSize(){
		setTabBoxHeight();
	}
	frameSize();
	$(function(){
		$(window).resize(function(){
	  		frameSize();
		});
	})	

	$(document).ready(lashen);
</script>

</body>
</html>
