<?xml version = '1.0' encoding = 'UTF-8'?>
<struts-config>
	<form-beans>
		<form-bean name="dynamicForm" type="com.cnpc.jcdp.mvc.action.DynaActionForm" />
	</form-beans>
	<action-mappings>
		<!--获取车辆维修费用-->
		<action path="/dms/repair/devRepaTypes" type="com.bgp.mcs.web.pm.gmsChart.action.querySeriesXMLDataAction"
			scope="request">
			<serviceCall serviceName="RepairStatAnalSrv" operationName="devRepaTypes" />
		</action>
		<!--获取维修费用统计图表数据-->
		<action path="/dms/repair/getRepairCostChartData" type="com.bgp.mcs.web.pm.gmsChart.action.querySeriesXMLDataAction"
			scope="request">
			<serviceCall serviceName="RepairStatAnalSrv" operationName="getRepairCostChartData" />
		</action>
		<!--获取维护,保养图表数据-->
		<action path="/dms/repair/getRepairMainChartData" type="com.bgp.mcs.web.pm.gmsChart.action.querySeriesXMLDataAction"
			scope="request">
			<serviceCall serviceName="RepairStatAnalSrv" operationName="getRepairMainChartData" />
		</action>
		<!--获取维修费用占比图表数据-->
		<action path="/dms/repair/getRepairCostProportionChartData" type="com.bgp.mcs.web.pm.gmsChart.action.querySeriesXMLDataAction"
			scope="request">
			<serviceCall serviceName="RepairStatAnalSrv" operationName="getRepairCostProportionChartData" />
		</action>
		<!--获取维修费用占比图表数据-->
		<action path="/dms/repair/getRepairItemChartData" type="com.bgp.mcs.web.pm.gmsChart.action.querySeriesXMLDataAction"
			scope="request">
			<serviceCall serviceName="RepairStatAnalSrv" operationName="getRepairItemChartData" />
		</action>
		<!--获取维修类别图表数据-->
		<action path="/dms/repair/getRepairTypeChartData" type="com.bgp.mcs.web.pm.gmsChart.action.querySeriesXMLDataAction"
			scope="request">
			<serviceCall serviceName="RepairStatAnalSrv" operationName="getRepairTypeChartData" />
		</action>
		<!--获取维修费用率图表数据-->
		<action path="/dms/repair/getRepairCostRateChartData" type="com.bgp.mcs.web.pm.gmsChart.action.querySeriesXMLDataAction"
			scope="request">
			<serviceCall serviceName="RepairStatAnalSrv" operationName="getRepairCostRateChartData" />
		</action>
		<!--获取设备费用率利润率图表数据（公司）-->
		<action path="/dms/repair/getDeviceCostProfitRateChartData" type="com.bgp.mcs.web.pm.gmsChart.action.querySeriesXMLDataAction"
			scope="request">
			<serviceCall serviceName="RepairStatAnalSrv" operationName="getDeviceCostProfitRateChartData" />
		</action>
		<!--获取设备费用率利润率图表数据（物探处）-->
		<action path="/dms/repair/getWuTanCostProfitRateChartData" type="com.bgp.mcs.web.pm.gmsChart.action.querySeriesXMLDataAction"
			scope="request">
			<serviceCall serviceName="RepairStatAnalSrv" operationName="getWuTanCostProfitRateChartData" />
		</action>
		<!--获取备件消耗统计图表数据-->
		<action path="/dms/repair/getSpareConsStatChartData" type="com.bgp.mcs.web.pm.gmsChart.action.querySeriesXMLDataAction"
			scope="request">
			<serviceCall serviceName="RepairStatAnalSrv" operationName="getSpareConsStatChartData" />
		</action>
		<!--获取备件消耗统计图表数据年度-->
		<action path="/dms/repair/getSpareConsStatChartDataYear" type="com.bgp.mcs.web.pm.gmsChart.action.querySeriesXMLDataAction"
			scope="request">
			<serviceCall serviceName="RepairStatAnalSrv" operationName="getSpareConsStatChartDataYear" />
		</action>
		<!--获取子类型备件消耗统计图表数据-->
		<action path="/dms/repair/getCSpareConsStatChartData" type="com.bgp.mcs.web.pm.gmsChart.action.querySeriesXMLDataAction"
			scope="request">
			<serviceCall serviceName="RepairStatAnalSrv" operationName="getCSpareConsStatChartData" />
		</action>
		<!--获取备件库存占比统计图表数据-->
		<action path="/dms/repair/getSpareStockChartData" type="com.bgp.mcs.web.pm.gmsChart.action.querySeriesXMLDataAction"
			scope="request">
			<serviceCall serviceName="RepairStatAnalSrv" operationName="getSpareStockChartData" />
		</action>
		<!--获取备件库存占比统计图表数据-->
		<action path="/dms/repair/getSpareConsumeChartData" type="com.bgp.mcs.web.pm.gmsChart.action.querySeriesXMLDataAction"
			scope="request">
			<serviceCall serviceName="RepairStatAnalSrv" operationName="getSpareConsumeChartData" />
		</action>
	</action-mappings>
</struts-config>
