	3:1206="19.0";1214="1";1220="100";1105="en=\"reportDate\",ch=\"上报日期\",tp=6,w=,fmt=,val=;en=\"pMonth\",ch=\"上报月份\",tp=6,w=,fmt=,val=;";1102="name=ds1,sql=\"select tt.*, tt2.org_abbreviation as org_name\\r\\n  from (select t.org_subjection_id,\\r\\n               sum(case t.item_name\\r\\n                     when \\\'资产折旧\\\' then\\r\\n                      cumu_report\\r\\n                   end) as zczj,\\r\\n               sum(case t.item_name\\r\\n                     when \\\'采购价格\\\' then\\r\\n                      cumu_report\\r\\n                   end) as cgjg,\\r\\n               sum(case t.item_name\\r\\n                     when \\\'库存\\\' then\\r\\n                      cumu_report\\r\\n                   end) as kc,\\r\\n               sum(case t.item_name\\r\\n                     when \\\'检波器电缆摊销折旧\\\' then\\r\\n                      cumu_report\\r\\n                   end) as jbqdltxzj,\\r\\n               sum(case t.item_name\\r\\n                     when \\\'设备维修\\\' then\\r\\n                      cumu_report\\r\\n                   end) as sbwx\\r\\n          from (select item.item_name,\\r\\n                       tmp2.org_subjection_id,\\r\\n                       tmp3.cumu_report\\r\\n                  from (select distinct conf1.item_id\\r\\n                          from dms_assess_jbzx_conf conf1\\r\\n                         where conf1.bsflag = \\\'0\\\') tmp1\\r\\n                 inner join dms_assess_indexconf_item item\\r\\n                    on tmp1.item_id = item.item_id\\r\\n                   and item.bsflag = \\\'0\\\'\\r\\n                  join (select distinct conf2.org_subjection_id\\r\\n                         from dms_assess_jbzx_conf conf2\\r\\n                        where conf2.bsflag = \\\'0\\\') tmp2\\r\\n                    on 1 = 1\\r\\n                  left join (select inf.org_subjection_id,\\r\\n                                   conf.item_id,\\r\\n                                   det.cumu_report\\r\\n     