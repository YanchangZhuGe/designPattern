	3:1206="19.0";1214="1";1220="100";1105="en=\"year\",ch=\"年度\",tp=6,w=,fmt=\"\",val=\"\";en=\"plan_type\",ch=\"需求类型\",tp=6,w=,fmt=\"\",val=\"\";en=\"purc_type\",ch=\"采购类型\",tp=6,w=,fmt=\"\",val=\"\";";1102="name=ds1,sql=\"SELECT tmp1.org_subjection_id,\\r\\n        tmp1.org_abbreviation,\\r\\n        tmp1.material_code,\\r\\n        tmp2.apply_number,\\r\\n        tmp2.apply_price,\\r\\n        tmp3.amount,\\r\\n        tmp3.price,\\r\\n        tmp4.order_amount,\\r\\n        tmp4.order_price  from (select t.org_subjection_id, inf.org_abbreviation, t.material_code\\r\\n           from (select distinct ad.org_subjection_id,\\r\\n                                 ad.material_code as material_code\\r\\n                   from dms_exeplan_apply_detail ad\\r\\n                   left join dms_exeplan_apply ap\\r\\n                     on ap.apply_id = ad.apply_id\\r\\n                  where ap.bsflag = \\\'0\\\' and ad.bsflag = \\\'0\\\' and ad.org_subjection_id is not null\\r\\n                 and ap.apply_year=?\\r\\n                 and ap.order_type=?\\r\\n                 union\\r\\n                 select distinct pd.org_subjection_id, pd.material_code\\r\\n                   from dms_exeplan_purc_detail pd\\r\\n                   left join dms_exeplan_purc pu\\r\\n                     on pu.purc_id = pd.purc_id\\r\\n                  where pu.bsflag = \\\'0\\\' and pd.bsflag = \\\'0\\\' and pd.org_subjection_id is not null\\r\\n                  and pu.purc_year=?\\r\\n                  and pu.order_type=?\\r\\nunion\\r\\n                 select distinct pd.org_subjection_id, pd.material_code\\r\\n                   from dms_exeplan_purc_detail pd\\r\\n                   left join dms_exeplan_purc pu\\r\\n                     on pu.purc_id = pd.purc_id\\r\\n                  where pu.bsflag = \\\'0\\\' and pd.bsflag = \\\'0\\\' and pd.org_subjection_id is not null\\r\\n                  and pu.purc_year=\\\'2017\\\'\\r\\n                  and pu.order_type=\\\'Z006\\\'\\r\\n             ) t\\r\\n           left join comm_org_subjection sub\\r\\n             on t.org_subjection_id = sub.org_subjection_id and sub.bsflag=\\\'0\\\'\\r\\n           left join comm_org_information inf\\r\\n             on sub.org_id = inf.org_id and inf.bsflag=\\\'0\\\'\\r\\n          where t.material_code is not null\\r\\n          order by sub.org_subjection_id, sub.coding_show_id\\r\\n         ) tmp1\\r\\n   left join (select ad.org_subjection_id,\\r\\n                     ad.material_code as material_code,\\r\\n                     sum(ad.apply_number) as apply_number,\\r\\n                     round(sum(ad.apply_number * ad.asse_price)/10000,3) as apply_price\\r\\n                from dms_exeplan_apply_detail ad\\r\\n                left join dms_exeplan_apply ap\\r\\n                  on ap.apply_id = ad.apply_id\\r\\n               where ad.bsflag = \\\'0\\\' and ap.bsflag = \\\'0\\\' and ad.org_subjection_id is not null\\r\\n              and ap.apply_year =?\\r\\n              and ap.order_type=?\\r\\n               group by ad.org_subjection_id, ad.material_code\\r\\n               order by ad.material_code) tmp2\\r\\n     on tmp1.org_subjection_id = tmp2.org_subjection_id\\r\\n    and tmp1.material_code = tmp2.material_code\\r\\n   left join (select pd.org_subjection_id,\\r\\n                     pd.material_code,\\r\\n                     sum(pd.amount) as amount,\\r\\n                     round(sum(pd.amount * pd.price)/10000,3) as price\\r\\n                from dms_exeplan_purc_detail pd\\r\\n                left join dms_exeplan_purc pu\\r\\n                  on pd.purc_id = pu.purc_id\\r\\n               where pd.bsflag = \\\'0\\\' and pu.bsflag = \\\'0\\\' and pd.org_subjection_id is not null\\r\\n              and pu.purc_year =?\\r\\n              and pu.order_type=?\\r\\n               group by pd.org_subjection_id, pd.material_code\\r\\n               order by pd.material_code\\r\\n              ) tmp3\\r\\n     on tmp1.org_subjection_id = tmp3.org_subjection_id\\r\\n    and tmp1.material_code = tmp3.material_code\\r\\nleft join (                \\r\\n       select to_char(to_date(p.create_date, \\\'yyyymmdd\\\'), \\\'yyyy-mm-dd\\\') as create_date,co.org_subjection_id as org_subjection_id,\\r\\n       t.material_code,\\r\\n       t.material_desc,\\r\\n       t.amount as order_amount,\\r\\n       t.amount_money as order_price\\r\\n   from dms_cg_apply_fp t\\r\\n   left join dms_cg_apply p \\r\\n   on p.cg_order_num = t.ebeln\\r\\n   left join comm_org_subjection co\\r\\n   on co.org_id = t.accept_org_id\\r\\n   where t.bsflag = \\\'0\\\'\\r\\n   and to_char(to_date(p.create_date, \\\'yyyymmdd\\\'), \\\'yyyy\\\') =?\\r\\n   order by t.create_date desc\\r\\n   ) tmp4\\r\\n   on tmp1.org_subjection_id = tmp4.org_subjection_id\\r\\n   and tmp1.material_code = tmp4.material_code\\r\\n   order by tmp1.org_subjection_id,tmp1.material_code\",type=normal,param=\"year:6;plan_type:6;year:6;purc_type:6;year:6;plan_type:6;year:6;purc_type:6;year:6\",cached=0,,indexSorted=false,indexIsUnique=false;name=ds2,sql=\"select distinct ad.org_subjection_id,\\r\\n                ad.material_code          as material_code,\\r\\n                ad.material_desc          as material_desc\\r\\n  from dms_exeplan_apply_detail ad\\r\\n  left join dms_exeplan_apply ap\\r\\n    on ap.apply_id = ad.apply_id\\r\\n where ap.bsflag = \\\'0\\\' and ad.bsflag = \\\'0\\\' and ad.org_subjection_id is not null\\r\\n   and ap.apply_year = ?\\r\\n   and ap.order_type = ?\\r\\nunion\\r\\nselect distinct pd.org_subjection_id, pd.material_code, pd.material_desc\\r\\n  from dms_exeplan_purc_detail pd\\r\\n  left join dms_exeplan_purc pu\\r\\n    on pu.purc_id = pd.purc_id\\r\\n where pu.bsflag = \\\'0\\\' and pd.bsflag = \\\'0\\\' and pd.org_subjection_id is not null\\r\\n   and pu.purc_year = ?\\r\\n   and pu.order_type = ?\",type=normal,param=\"year:6;plan_type:6;year:6;purc_type:6\",cached=0,,indexSorted=false,indexIsUnique=fals