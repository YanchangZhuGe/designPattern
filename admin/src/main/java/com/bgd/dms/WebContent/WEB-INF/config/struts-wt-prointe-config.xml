<?xml version = '1.0' encoding = 'UTF-8'?>
<struts-config>
	<form-beans>
		<form-bean name="dynamicForm" type="com.cnpc.jcdp.mvc.action.DynaActionForm" />
	</form-beans>
	<action-mappings>
		<!-- 保存或修改任务书 -->
		<action path="/wt/prointe/saveOrUpdateTaskBookWt" type="com.cnpc.jcdp.mvc.action.WSAction" >
			<serviceCall serviceName="WtProinteSrv" operationName="saveOrUpdateTaskBookWt" />
		</action>		
		<!-- 保存或修改项目人员信息 -->
		<action path="/wt/prointe/saveOrUpdatePerjPersonWt" type="com.cnpc.jcdp.mvc.action.WSAction" >
			<serviceCall serviceName="WtProinteSrv" operationName="saveOrUpdatePerjPersonWt" />
		</action>
		

		
		<!-- 综合物化探处理解释设计新增 -->
		
		<action path="/wt/prointe/saveOrUpdateHandle" type="com.cnpc.jcdp.mvc.action.WSAction" >
			<serviceCall serviceName="WtProinteSrv"
				operationName="saveOrUpdateHandle" />
	 		<forward name="success" path="/wt/prointe/handle_design.jsp"/>
		</action>

	</action-mappings>
	
</struts-config>
