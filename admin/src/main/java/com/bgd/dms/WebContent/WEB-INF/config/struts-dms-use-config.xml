<?xml version = '1.0' encoding = 'UTF-8'?>
<struts-config>
	<form-beans>
		<form-bean name="dynamicForm" type="com.cnpc.jcdp.mvc.action.DynaActionForm" />
	</form-beans>
	<action-mappings>
		<!--查询公司级地震仪器动态情况统计表 -->
		<action path="/dms/use/getCompEqChart" type="com.bgp.gms.service.rm.dm.action.DevFusionChartsDataAction"
			scope="request" name="dynamicForm">
			<serviceCall serviceName="DzyqStatSrv" operationName="getCompEqChart" />
		</action>
		<!--查询公司级在用的统计数据 -->
		<action path="/dms/use/getEqUseComDistribute" type="com.bgp.gms.service.rm.dm.action.DevFusionChartsDataAction"
			scope="request" name="dynamicForm">
			<serviceCall serviceName="DzyqStatSrv" operationName="getEqUseComDistribute" />
		</action>
		<!--查询公司级闲置的统计数据 -->
		<action path="/dms/use/getEqUnuseComDistribute" type="com.bgp.gms.service.rm.dm.action.DevFusionChartsDataAction"
			scope="request" name="dynamicForm">
			<serviceCall serviceName="DzyqStatSrv" operationName="getEqUnuseComDistribute" />
		</action>
		<!--获取机构利用率对比图表数据  start-->
		<action path="/dmsManager/use/devOrgUseRate/getUseRateForOrg" type="com.bgp.mcs.web.pm.gmsChart.action.querySeriesXMLDataAction"
			scope="request">
			<serviceCall serviceName="DeviceUseSrv" operationName="getUseRateForOrg" />
		</action> 
		
		<!--获取利用率对比图表数据  start-->
		<action path="/dms/device/getUseRateForOrg" type="com.bgp.mcs.web.pm.gmsChart.action.querySeriesXMLDataAction"
			scope="request">
			<serviceCall serviceName="DeviceUseRateSrv" operationName="getUseRateForOrg" />
		</action> 
		<!--获取利用率对比图表数据  start-->
		<action path="/dms/device/getUseRateNew" type="com.bgp.mcs.web.pm.gmsChart.action.querySeriesXMLDataAction"
			scope="request">
			<serviceCall serviceName="DeviceUseRateSrv" operationName="getUseRateNew" />
		</action>
		<!--获取利用率完好率图表数据(合并)-->
		<action path="/dms/use/getDeviceUseAndWhole" type="com.bgp.mcs.web.pm.gmsChart.action.querySeriesXMLDataAction"
			scope="request">
			<serviceCall serviceName="DeviceUseAndWholeSrv" operationName="getUseRate" />
		</action> 
		
			<!--设备资源池报表 start-->
		<action path="/dms/device/getDeviceLie" type="com.bgp.mcs.web.pm.gmsChart.action.querySeriesXMLDataAction"
			scope="request">
			<serviceCall serviceName="DeviceUseRateSrv" operationName="getDeviceLie" />
		</action> 
		
			<!--设备资源池检波器二级钻取报表 start-->
		<action path="/dms/device/getSecondJBQLie" type="com.bgp.mcs.web.pm.gmsChart.action.querySeriesXMLDataAction"
			scope="request">
			<serviceCall serviceName="DeviceUseRateSrv" operationName="getSecondJBQLie" />
		</action> 
			<!--设备资源池按照物探处钻取报表 start-->
		<action path="/dms/device/getFourJBQLie" type="com.bgp.mcs.web.pm.gmsChart.action.querySeriesXMLDataAction"
			scope="request">
			<serviceCall serviceName="DeviceUseRateSrv" operationName="fourLevelJBQLie" />
		</action> 
			<!--设备资源池报表 start-->
		<action path="/dms/device/getDeviceLieOne" type="com.bgp.mcs.web.pm.gmsChart.action.querySeriesXMLDataAction"
			scope="request">
			<serviceCall serviceName="DeviceUseRateSrv" operationName="getDeviceLieOne" />
		</action> 
		<!-- 获取资产创收创效数据(公司) -->
		<action path="/dms/use/getAssetsIncomeOrProfitData" type="com.bgp.mcs.web.pm.gmsChart.action.querySeriesXMLDataAction"
			scope="request">
			<serviceCall serviceName="AssetsIncomeSrv" operationName="getAssetsIncomeOrProfitData" />
		</action> 
		<!-- 获取资产创收创效数据(物探处) -->
		<action path="/dms/use/getWutanAssetsIncomeOrProfitData" type="com.bgp.mcs.web.pm.gmsChart.action.querySeriesXMLDataAction"
			scope="request">
			<serviceCall serviceName="AssetsIncomeSrv" operationName="getWutanAssetsIncomeOrProfitData" />
		</action>
		
	</action-mappings>
</struts-config>
