<?xml version = '1.0' encoding = 'UTF-8'?>
<struts-config>
	<form-beans>
		<form-bean name="dynamicForm" type="com.cnpc.jcdp.mvc.action.DynaActionForm" />
	</form-beans>
	<action-mappings>

		<!-- test -->


		<action path="/pm/bpm/toStartExamine" type="com.cnpc.jcdp.mvc.action.WSAction" scope="request">
			<serviceCall serviceName="workFlowSrv" operationName="startTestExamine"  />
			<forward name="success" path="/docManage/comm_doc_info.jsp" />
		</action>
		<action path="/pm/bpm/toViewExamineInfo" type="com.cnpc.jcdp.mvc.action.WSAction" scope="request">
			<serviceCall serviceName="workFlowSrv" operationName="viewTestExamine" />
			<forward name="success" path="/pm/bpm/test/gmsFileInfo.jsp" />
		</action>
		<action path="/pm/bpm/toDoExamineInfo" type="com.cnpc.jcdp.mvc.action.WSAction" scope="request">
			<serviceCall serviceName="workFlowSrv" operationName="doTestExamine" />
			<forward name="success" path="/pm/bpm/test/gmsFileAuditList.jsp"  redirect="true"/>
		</action>

		<action path="/pm/op/bpm/toViewExamineInfo" type="com.cnpc.jcdp.mvc.action.WSAction" scope="request">
			<serviceCall serviceName="WFCommonSrv" operationName="toViewExamineInfowfpg" />
			<forward name="success" path="/pm/bpm/test/opBpmTestInfo.jsp"/>
		</action>
		
		<action path="/pm/op/bpm/toDoExamineInfo" type="com.cnpc.jcdp.mvc.action.WSAction" scope="request">
			<serviceCall serviceName="WFCommonSrv" operationName="toDoExamineInfowfpa" />
			<forward name="success" path="/bpm/common/toGetProcessList.srq"/>
		</action>
		
		<!-- common -->
		<action path="/bpm/common/toStartProcess" type="com.cnpc.jcdp.mvc.action.WSAction" scope="request">
			<serviceCall serviceName="WFCommonSrv" operationName="startWFProcess"  />
			<forward name="success" path="/pm/bpm/test/gmsFileList.lpmd" />
		</action>
		<action path="/bpm/common/toGetProcessList" type="com.cnpc.jcdp.mvc.action.WSAction" scope="request">
			<serviceCall serviceName="WFCommonSrv" operationName="getCustomConfigInfo"  />
			<forward name="success" path="/pm/bpm/singleProcess/commonProcessList.jsp" />
		</action>
		<action path="/bpm/common/toGetSelfProcessList" type="com.cnpc.jcdp.mvc.action.WSAction" scope="request">
			<serviceCall serviceName="WFCommonSrv" operationName="getCustomConfigInfo"  />
			<forward name="success" path="/pm/bpm/singleProcess/commonSelfProcessList.jsp" />
		</action>
			<action path="/bpm/common/commonList" type="com.cnpc.jcdp.mvc.action.WSAction" scope="request">
			<serviceCall serviceName="WFCommonSrv" operationName="getCustomConfigInfo"  />
			<forward name="success" path="/pm/bpm/singleProcess/commonList.jsp" />
		</action>
		<action path="/bpm/common/toProcessDashbordList" type="com.cnpc.jcdp.mvc.action.WSAction" scope="request">
			<serviceCall serviceName="WFCommonSrv" operationName="getCustomConfigInfo"  />
			<forward name="success" path="/pm/bpm/singleProcess/commonProcessDashbord.jsp" />
		</action>
		<action path="/bpm/common/toGetProcessInfo" type="com.cnpc.jcdp.mvc.action.WSAction" scope="request">
			<serviceCall serviceName="WFCommonSrv" operationName="getWFProcessInfo"  />
			<forward name="success" path="/pm/bpm/singleProcess/commonProcessInfo.jsp" />
		</action>
		<action path="/bpm/common/toGetProcessInfoView" type="com.cnpc.jcdp.mvc.action.WSAction" scope="request">
			<serviceCall serviceName="WFCommonSrv" operationName="getWFProcessInfo"  />
			<forward name="success" path="/pm/bpm/singleProcess/commonProcessInfoView.jsp" />
		</action>
		<action path="/bpm/common/toDoProcessInfo" type="com.cnpc.jcdp.mvc.action.WSAction" scope="request">
			<serviceCall serviceName="WFCommonSrv" operationName="doWfProcess"  />
			<forward name="success" path="/pm/bpm/common/close_page.jsp"  redirect="true"/>
		</action>
		
		<action path="/dashbordSrv/getDashbordInfos" type="com.cnpc.jcdp.mvc.action.WSAction" scope="request">
			<serviceCall serviceName="DashbordSrv" operationName="getDashbordInfos"  />
			<forward name="success" path="/dashboardIndex.jsp" />
		</action>
		
		<action path="/bpm/common/getProcTypeTree" type="com.bgp.mcs.web.pm.p6.resource.resourceAssignment.GetTaskAction"
			scope="request">
			<serviceCall serviceName="WFCommonSrv"
				operationName="getProcTypeTree" />
		</action>
		
		<action path="/bpm/common/saveWFDocInfo" type="com.cnpc.jcdp.mvc.action.jcdp.WSUploadAction"
			scope="request" name="dynamicForm">
			<serviceCall serviceName="WFCommonSrv" operationName="saveWFDocInfo" />
		</action>
		
		<action path="/bpm/common/updateWfRProcinst" type="com.cnpc.jcdp.web.auth.action.AddOrUpdateUserAction"
			scope="request">
			<serviceCall serviceName="IBPLoginAndMenuTree"
				operationName="updateWfRProcinst" />
		</action>
		<!-- 大港 -->
		<action path="/bpm/common/toGetSelfProcessListDg" type="com.cnpc.jcdp.mvc.action.WSAction" scope="request">
			<serviceCall serviceName="WFCommonSrv" operationName="getCustomConfigInfoDg"  />
			<forward name="success" path="/mat/singleproject/plansub/planSubListDgwt.jsp" />
		</action>
		
		<!-- 大港自有-设备 -->
		<action path="/bpm/common/toGetSelfProcessListDevDgOwn" type="com.cnpc.jcdp.mvc.action.WSAction" scope="request">
			<serviceCall serviceName="WFCommonSrv" operationName="getCustomConfigInfoDg"  />
			<forward name="success" path="/rm/dm/devPlanMulti/ownEqDevApprove_dg.jsp" />
		</action>
		<!-- 大港外租-设备 -->
		<action path="/bpm/common/toGetSelfProcessListDevDgHire" type="com.cnpc.jcdp.mvc.action.WSAction" scope="request">
			<serviceCall serviceName="WFCommonSrv" operationName="getCustomConfigInfoDg"  />
			<forward name="success" path="/rm/dm/devPlanMulti/hireEqDevApprove_dg.jsp" />
		</action>
		<!-- 大港专业化-设备 -->
		<action path="/bpm/common/toGetSelfProcessListDevDgPro" type="com.cnpc.jcdp.mvc.action.WSAction" scope="request">
			<serviceCall serviceName="WFCommonSrv" operationName="getCustomConfigInfoDg"  />
			<forward name="success" path="/rm/dm/devPlanMulti/proEqDevApprove_dg.jsp" />
		</action>
		<!-- 大港调剂-设备 -->
		<action path="/bpm/common/toGetSelfProcessListDevDgMix" type="com.cnpc.jcdp.mvc.action.WSAction" scope="request">
			<serviceCall serviceName="WFCommonSrv" operationName="getCustomConfigInfoDg"  />
			<forward name="success" path="/rm/dm/devPlanMulti/mixDevApprove_dg.jsp" />
		</action>
	</action-mappings>
</struts-config>
