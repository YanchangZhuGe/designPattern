<?xml version="1.0" encoding="GBK"?>

<SAIS_EAW_TL>
  <PagerAction>Edit</PagerAction>
  <PageTitle>操作</PageTitle>
  <QuerySQL>
    <sql>SELECT * FROM sys_config_operation_dms WHERE opt_id='$PARAM.id'</sql>
    <tableName>sys_config_operation_dms</tableName>
    <fields>
      <field name="opt_id" type="TEXT">opt_id</field>
      <field name="service_id" type="TEXT">service_id</field>
      <field name="opt_name" type="TEXT">opt_name</field>
      <field name="opt_cnname" type="TEXT">opt_cnname</field>
      <field name="is_monitor" type="TEXT">is_monitor</field>
      <field name="is_log" type="TEXT">is_log</field>
      <field name="create_time" type="D">create_time</field>
      <field name="creator" type="TEXT">creator</field>
      <field name="memo" type="TEXT">memo</field>
      <field name="impl_type" type="TEXT">impl_type</field>
      <field name="query_sql" type="TEXT">query_sql</field>
      <field name="info_tag" type="TEXT">info_tag</field>
      <field name="field_map" type="TEXT">field_map</field>
      <field name="data_auth_key" type="TEXT">data_auth_key</field>
    </fields>
  </QuerySQL>
  <Options>
    <Select name="isOps">
      <option value="0">否</option>
      <option value="1">是</option>
    </Select>
    <Select name="implType">
      <option value="OPT_TYPE_NORMAL">普通操作</option>
      <option value="OPT_TYPE_ONE_Q">单行查询</option>
      <option value="OPT_TYPE_MUTI_Q">分页查询</option>
      <option value="OPT_TYPE_ALL_Q">不分页查询</option>
    </Select>    
  </Options>
  <Codes/>
  <EntityFields>
    <field name="opt_id" isShow="Hide"/>
    <field name="opt_name" nonEmpty="true">操作名</field>
    <field name="opt_cnname">中文名称</field>
    <field name="impl_type" type="SEL_OPs" selValue="implType">操作类型</field>
    <field name="info_tag">Info Tag</field>     
    <field name="is_log" type="SEL_OPs" selValue="isOps">记录日志</field>
    <field name="is_monitor" type="SEL_OPs" selValue="isOps">监控服务</field>   
    <field name="query_sql" type="MEMO" size="65,6">查询SQL</field>    
  </EntityFields>
</SAIS_EAW_TL>
