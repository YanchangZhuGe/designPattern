      <field name="vsp_sign_sum" type="">VSP_SIGN_SUM</field>
      <field name="vsp_intending_workload" type="">VSP_INTENDING_WORKLOAD</field>
      <field name="vsp_finished_workload" type="">VSP_FINISHED_WORKLOAD</field>
      <field name="ipce_area" type="TEXT">IPCE_AREA</field>
      <field name="design_start_date" type="D">DESIGN_START_DATE</field>
      <field name="design_end_date" type="D">DESIGN_END_DATE</field>
      <field name="project_business_type" type="TEXT">PROJECT_BUSINESS_TYPE</field>
      <field name="prctr" type="TEXT">PRCTR</field>
    </fields>
  </QuerySQL>
  <Options>
    <Select name="country">
      <option value="1">国内</option>
      <option value="2">国外</option>
    </Select>
  </Options>
  <Codes/>
  <EntityFields>
    <field name="project_info_no" isShow="Hide">PROJECT_INFO_NO</field>
    <field name="project_name">项目名称</field>
    <field name="manage_org" type="FK"  isShow="ReadOnly" defaultValue="$ENTITY.manage_org:$ENTITY.coding_name" selValue="orgList2Link.lpmd">管理机构</field>
    <field name="modifi_date" type="D" action="R,U">修改时间</field>
    <field name="project_country" type="SEL_OPs" selValue="country">项目范围</field>
    <field name="notes" type="MEMO">备注</field>
  </EntityFields>
</SAIS_EAW_TL>
