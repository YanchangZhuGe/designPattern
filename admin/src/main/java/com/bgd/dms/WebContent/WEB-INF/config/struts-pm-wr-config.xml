<?xml version = '1.0' encoding = 'UTF-8'?>
<struts-config>

  <action-mappings>       
	     <action path="/pm/wr/getCompanyProductionInfo" type="com.bgp.mcs.web.pm.wr.weekPPTPic.WSJfreeChartAction"
			scope="request">
			<serviceCall serviceName="WeekRaoPicChartSrv"
				operationName="getCompanyProductionInfo" />
			<forward name="success" path="/pm/wr/weekRaoPPT/weekRaoPic.jsp"/>
		</action>
		
		<action path="/pm/wr/getCompanyProductionInfoBySubflag" type="com.cnpc.jcdp.mvc.action.WSAction"
			scope="request">
			<serviceCall serviceName="WeekRaoPicChartSrv"
				operationName="getWeekPrintSubflagInfo" />
			<forward name="success" path="/pm/hq/productionControlWeekPrint.jsp"/>
		</action>
		
		<!-- 工作流审批页面 -->
		<action path="/pm/wr/getWeekPrintAudit" type="com.cnpc.jcdp.mvc.action.WSAction"
			scope="request">
			<serviceCall serviceName="WeekRaoPicChartSrv"
				operationName="getWeekPrintAudit" />
			<forward name="success" path="/pm/hq/productionControlWeekAudit.jsp"/>
		</action>
		
		<!-- 工作流审批查看页面 -->
		<action path="/pm/wr/getWeekPrintView" type="com.cnpc.jcdp.mvc.action.WSAction"
			scope="request">
			<serviceCall serviceName="WeekRaoPicChartSrv"
				operationName="getWeekPrintAudit" />
			<forward name="success" path="/pm/hq/productionControlWeekView.jsp"/>
		</action>
		
		<!-- 工作流审批保存页面 -->
		<action path="/pm/wr/toSavetWeekPrintAudit" type="com.cnpc.jcdp.mvc.action.WSAction"
			scope="request">
			<serviceCall serviceName="WeekRaoPicChartSrv"
				operationName="saveWeekPrintAudit" />
			<forward name="success" path="/pm/wr/wrReportAudit.jsp"/>
		</action>
		
		<action path="/pm/wr/getCompanyWordLoadInfo" type="com.bgp.mcs.web.pm.wr.weekPPTPic.WSJfreeChartWorkloadAction"
			scope="request">
			<serviceCall serviceName="WeekRaoPicChartSrv"
				operationName="getCompanyWorkloadInfo" />
			<forward name="success" path="/pm/wr/weekRaoPPT/weekRaoWorkLoadPic.jsp"/>
		</action>
		
		<!-- 地震采集项目信息导入-->
		<action path="/pm/wr/importExcelTemplateTest" type="com.cnpc.jcdp.mvc.action.jcdp.WSUploadAction"
			scope="request" name="dynamicForm">
			<serviceCall serviceName="DataExtractSrv"
				operationName="importExcelTemplate" />
				 <forward name="success" path="/pm/wr/projectDynamic/DynamicImport.jsp"/>
		</action>
  </action-mappings>  
</struts-config>
